doi = {10.1093/jxb/ers258},
file = {:Users/afragasso/Library/Application Support/Mendeley Desktop/Downloaded/Tamura, Hara-Nishimura - 2013 - The molecular architecture of the plant nuclear pore complex.pdf:pdf},
