issn = {02637855},
journal = {Journal of Molecular Graphics},
keywords = {Interactive visualization,Molecular dynamics visualization,Molecular modeling},
number = {1},
pages = {33--38},
pmid = {8744570},
title = {{VMD: Visual molecular dynamics}},
volume = {14},
year = {1996}
}
