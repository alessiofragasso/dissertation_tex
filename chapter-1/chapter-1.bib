}
@article{Wang2014,
abstract = {A label-free method for the measurement of the activity of HIV-1 protease is developed by real-time monitoring of the cleavage of a peptide substrate by HIV-1 protease in a nanopore. The method is rapid and sensitive: picomolar concentrations of HIV-1 protease could be detected in $\sim$10min. Simulated clinical samples are analyzed, and the activity of HIV-1 protease could be accurately detected. Our developed nanopore sensor design strategy should find useful applications in the development of stochastic sensors for other proteases of medical, pharmaceutical, and biological importance.},
author = {Wang, Liang and Han, Yujing and Zhou, Shuo and Guan, Xiyun},
doi = {https://doi.org/10.1016/j.bios.2014.06.041},
issn = {0956-5663},
