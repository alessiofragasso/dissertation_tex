file = {:Users/afragasso/Library/Application Support/Mendeley Desktop/Downloaded/Chauhan et al. - 2008 - Self-assembly of the 3-aminopropyltrimethoxysilane multilayers on Si and hysteretic current-voltage characterist.pdf:pdf},
isbn = {9122255052},
issn = {09478396},
journal = {Applied Physics A: Materials Science and Processing},
number = {3},
pages = {581--589},
title = {{Self-assembly of the 3-aminopropyltrimethoxysilane multilayers on Si and hysteretic current-voltage characteristics}},
