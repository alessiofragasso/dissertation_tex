file = {:Users/afragasso/Library/Application Support/Mendeley Desktop/Downloaded/Ebner, Hinterdorfer, Gruber - 2007 - Comparison of different aminofunctionalization strategies for attachment of single antibodies to AF.pdf:pdf},
issn = {03043991},
journal = {Ultramicroscopy},
keywords = {AFM,Avidin/biotin interaction,Force spectroscopy,Molecular recognition,Silanization},
number = {10-11},
pages = {922--927},
title = {{Comparison of different aminofunctionalization strategies for attachment of single antibodies to AFM cantilevers}},
volume = {107},
year = {2007}
}
