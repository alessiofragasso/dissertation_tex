issn = {01694332},
journal = {Applied Surface Science},
keywords = {Poly(N-isopropylacrylamide),Polymer brushes,Surface initiated polymerization,Surface plasmon resonance},
pages = {384--392},
publisher = {Elsevier B.V.},
title = {{Surface plasmon resonance methodology for monitoring polymerization kinetics and morphology changes of brushes—evaluated with poly(N-isopropylacrylamide)}},
url = {http://dx.doi.org/10.1016/j.apsusc.2016.10.165},
volume = {396},
year = {2017}
